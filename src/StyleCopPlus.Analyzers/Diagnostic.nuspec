<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/08/nuspec.xsd">
  <metadata>
    <id>StyleCopPlus.Analyzers</id>
    <version>1.2.1.0</version>
    <title>StyleCopPlus.Analyzers</title>
    <authors>Sergey Rybalkin</authors>
    <owners>Sergey Rybalkin</owners>
    <projectUrl>https://github.com/sergey-rybalkin/StyleCopPlus.Analyzers</projectUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>A subset of StyleCopPlus analyzers implemented using Roslyn engine.</description>
    <releaseNotes>Initial version.</releaseNotes>
    <copyright>Copyright (c) 2016</copyright>
    <tags>StyleCopPlus</tags>
    <frameworkAssemblies>
      <frameworkAssembly assemblyName="System" targetFramework="" />
    </frameworkAssemblies>
  </metadata>
  <!-- The convention for analyzers is to put language agnostic dlls in analyzers\portable50 and language specific analyzers in either analyzers\portable50\cs or analyzers\portable50\vb -->
  <files>
    <file src="*.dll" target="analyzers\dotnet\cs" exclude="**\Microsoft.CodeAnalysis.*;**\System.Collections.Immutable.*;**\System.Reflection.Metadata.*;**\System.Composition.*" />
    <file src="tools\*.ps1" target="tools\" />
  </files>
</package>